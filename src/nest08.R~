library(bipartite)
source('../src/pbr_load_data_hoth.R')
prn.08 <- list(c=pbr.08$c,x=pbr.08$x,cnpb=pbr.08$c[,colnames(pbr.08$c)!='pb'],xnpb=pbr.08$x[,colnames(pbr.08$x)!='pb'])
                                        #run nestedness
                                        #write results
print('2008 Control')
nest08c <- oecosimu(prn.08$c,nestfun=nested,method='r1')
write.csv(unlist(nest08c$oecosimu),file='../results/nest08c.results',row.names=FALSE)
print('2008 Exclusion')
nest08x <- oecosimu(prn.08$x,nestfun=nested,method='r1')
write.csv(unlist(nest08x$oecosimu),file='../results/nest08x.results')
print('2008 Control no pb')
nest08cnpb <- oecosimu(prn.08$cnpb,nestfun=nested,method='r1')
write.csv(unlist(nest08cnpb$oecosimu),file='../results/nest08cnpb.results')
print('2008 Exclusion no pb')
nest08xnpb <- oecosimu(prn.08$xnpb,nestfun=nested,method='r1')
write.csv(unlist(nest08xnpb$oecosimu),file='../results/nest08xnpb.results')
